extends ../_index.pug

block state
  - const scripts = ['../index.min.js']
  - const styles = ['../index.min.css']
  - const title = 'Option Picker'

block script
  script
    | document.querySelectorAll('input').forEach(input => {
    |     let picker = new OptionPicker(input);
    |     picker.on('change', (e, v) => console.log(['change', v]));
    |     picker.on('search', (e, v) => console.log(['search', v]));
    | });

block style
  style
    | .option-picker {
    |   min-width: 10em;
    | }
    | .option-picker__options {
    |   background: #fff;
    | }

block content
  main
    form(method='get' target='_blank')
      p
        input(list='colors' name='color' placeholder='Select a colorâ€¦' type='text' value='Blue')
      p
        button(type='submit') Submit
